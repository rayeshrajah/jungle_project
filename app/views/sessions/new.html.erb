<header class="page-header">
<h1>Login</h1>  
</header>

<div class="form-group">
<%= form_tag '/login' do %>
    <%= label :email, 'Email:' %> 
    <%= text_field_tag :email, "", class: 'form-control' %>
    <%= label :password, 'Password:' %> 
    <%= password_field_tag :password, "", class: 'form-control' %>
</div>
    <%= submit_tag "Submit", class: 'btn btn-primary'%>
<% end %>